{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=21.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build --force",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev.databases": "docker-compose up -d",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node --env-file=.env server/entry.express",
    "start": "vite --open --mode ssr",
    "test": "npx jest",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.15",
    "@builder.io/qwik-city": "^1.2.15",
    "@faker-js/faker": "^8.3.1",
    "@qwik-ui/headless": "^0.1.22",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.44.4",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.4",
    "@types/sanitize-html": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.15",
    "express": "4.18.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "valibot": "^0.21.0",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@modular-forms/qwik": "^0.21.1",
    "@quasarwork/qwik-city-nprogress": "^0.0.3",
    "@qwikest/icons": "^0.0.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/morgan": "^1.9.9",
    "@unpic/qwik": "^0.0.32",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "meilisearch": "^0.35.0",
    "morgan": "^1.10.0",
    "redis": "^4.6.13",
    "sanitize-html": "^2.11.0",
    "tailwind-merge": "^2.2.1",
    "vite-plugin-run": "^0.5.1"
  }
}
