{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.meilisearch": "source .env; docker run -d -it --name tda-meilisearch -e MEILI_MASTER_KEY=\"$MEILISEARCH_MASTER_KEY\" -p 7700:7700 -e MEILI_ENV='development' -v $(pwd)/tmp/meili_data:/meili_data getmeili/meilisearch:v1.4",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.fastify",
    "start": "vite --open --mode ssr",
    "test": "npx jest",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.15",
    "@builder.io/qwik-city": "^1.2.15",
    "@fastify/compress": "^6.2.1",
    "@fastify/static": "^6.10.1",
    "@types/eslint": "^8.44.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.15",
    "fastify": "^4.17.0",
    "fastify-plugin": "^4.5.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "vite": "^4.4.11",
    "vite-plugin-run": "^0.5.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "meilisearch": "^0.35.0"
  }
}
